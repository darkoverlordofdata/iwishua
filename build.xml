<?xml version="1.0" encoding="UTF-8"?>
<project name="Cake++"  basedir=".">
<!--
    Cake Runner
    
    ant task wrapper for netbeans tool menu
    -->

<target name="build">
    <exec executable="cake">
        <arg value="build"/>
    </exec>
</target>

<target name="publish">
    <exec executable="cake">
        <arg value="publish"/>
    </exec>
</target>

<target name="bower">
    <exec executable="bower">
        <arg value="install"/>
    </exec>
</target>

<target name="npm">
    <exec executable="npm">
        <arg value="install"/>
    </exec>
</target>

</project>
